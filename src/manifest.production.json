{
  "manifest_version": 2,
  "name": "1inch Extension",
  "version": "0.1.0",
  "description": "1inch App in your browser",
  "icons": {
    "16": "icons/icon-16x16.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "permissions": [
    "storage",
    "notifications"
  ],
  "browser_action": {
    "default_popup": "index.html"
  }
}